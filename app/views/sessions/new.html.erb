		<div class="text_fields">
			<div><%= f.label :login_pw, '密碼：' %></div>
			<div><%= f.password_field :login_pw %></div>
		</div>
	
		<%= submit_tag "Login", :id => "submit" %>
	
	<% end -%>
<% end -%>